#include <iostream>
using namespace std;
int main()
{
    int A,B,C;
    cout<<"ingresa un valor de A: ";
    cin>>A;
    if((A>=0)&&(A<=10))
    {
        cout<<"ingresa un valor de B: ";
        cin>>B;
        if((B>=0)&&(B<=10))
        {
           cout<<"ingresa un valor de C: ";
           cin>>C;
           if((C>=0)&&(C<=10)) 
           cout<<"el programa es: "<<(A+B*C)/3.0;
           else
           cout<<"error"<<C<<"no esta en el rango"<<endl;
        }
        else
        cout<<"error"<<B<<"no esta en el rango"<<endl;
    }
    else
     cout<<"error"<<A<<"no esta en el rango"<<endl;

    return 0;
}
